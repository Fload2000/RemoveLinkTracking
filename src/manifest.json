{
    "manifest_version": 2,
    "name": "RemoveLinkTracking",
    "version": "1.0.4",
    "description": "Removes tracking parameter in links",
    "homepage_url": "https://github.com/Fload2000/RemoveLinkTracking",
    "permissions": ["webRequest", "webRequestBlocking", "storage", "<all_urls>"],
    "browser_action": {
        "browser_style": true,
        "default_icon": {
            "16": "icon16.gif",
            "64": "icon.svg"
        },
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html"
    },
    "background": {
        "scripts": ["remover.js", "background.js"]
    }
}
