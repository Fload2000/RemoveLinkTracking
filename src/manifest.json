{
    "manifest_version": 2,
    "name": "RemoveLinkTracking",
    "version": "1.1.0",
    "description": "Removes tracking parameter in links",
    "homepage_url": "https://github.com/Fload2000/RemoveLinkTracking",
    "permissions": ["webRequest", "webRequestBlocking", "storage", "tabs", "<all_urls>"],
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "64": "img/icon64.png",
        "96": "img/icon96.png"
    },
    "browser_action": {
        "browser_style": true,
        "default_icon": {
            "16": "img/icon16.png",
            "48": "img/icon48.png",
            "64": "img/icon64.png",
            "96": "img/icon96.png"
        }
    },
    "options_ui": {
        "page": "options.html"
    },
    "background": {
        "scripts": ["remover.js", "background.js"]
    }
}
